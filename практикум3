# объявление функции
def is_password_good(password):
    count=0
    if len(txt)>7:
        count +=1
    if any([x.isupper() for x in password])==True:
        count +=1
    if any([x.islower() for x in password])==True:
        count +=1
    if any([x.isdigit() for x in password])==True:
        count +=1
    return count==4

# считываем данные
txt = input()

# вызываем функцию
print(is_password_good(txt))
